{
    "version": 2,
    "builds": [
      {
        "src": "src/server/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/index.jsx",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/instagram",
        "dest": "src/backend/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "src/index.jsx"
      }
    ]
  }