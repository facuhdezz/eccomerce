{
    "version": 2,
    "builds": [
      {
        "src": "src/server/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/index.jsx",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }